{
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
    "dictionaries": ["technical", "dbd"],
    "dictionaryDefinitions": [
        {
            "addWords": true,
            "name": "technical",
            "path": "./cspell/technical.txt"
        },
        {
            "addWords": true,
            "name": "dbd",
            "path": "./cspell/dbd.txt"
        }
    ],
    "ignorePaths": [
        ".next",
        "node_modules",
        "pnpm-lock.yaml",
        ".vscode",
        "src/__generated__"
    ],
    "words": ["Jarjour"]
}
